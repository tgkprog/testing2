# json/nsxml/xml/plain
format=json

# 0/1
array.enable=1

# min/max/eq
# 1/5/2
# min/1, max/5, eq/2
array.count=min/5

lookFor1=agreementId
lookFor2=applecareSalesDate
lookFor3=cancellationDate
lookFor4=customerEmailId
lookFor5=deviceId
lookFor6=monthlyPayment
lookFor7=poNumber
lookFor8=productDescription


//agreementId
lookFor1.typ1=Numeric

//applecareSalesDate
lookFor2.typ1=DateFormat
lookFor2.typ1.p1=mm/dd/yy

//cancellationDate
lookFor3.typ1=DateFormat
lookFor3.typ1.p1=mm/dd/yy

//customerEmailId
# 
lookFor4.xPath=deviceRequest/deviceRequest1/customerEmailId
# warn/error/ignore
lookFor4.xPath.ifmissing=warn

lookFor4.typ1=EmailVerify
lookFor4.typ1.p1=TYPE_SIMPLE

//deviceId
lookFor5.array.cnt=1
lookFor5.arrayIns.cnt=3
lookFor5.array.ins1.typ1=AlphaNumeric
lookFor5.array.ins1.typ1=AlphaNumeric
lookFor5.array.ins1.typ2.p1=46523

